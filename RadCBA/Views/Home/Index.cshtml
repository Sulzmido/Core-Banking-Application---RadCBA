                        <div class="h5 mb-0 font-weight-bold text-gray-800">@ViewBag.TellerPostApprovedCount</div>
                    </div>
                    <div class="col-auto">
                        <i class="fas fa-check-square fa-2x text-gray-300"></i>
                    </div>
                </div>
